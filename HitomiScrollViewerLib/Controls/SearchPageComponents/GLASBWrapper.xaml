<?xml version="1.0" encoding="utf-8"?>
<StackPanel
    x:Class="HitomiScrollViewerLib.Controls.SearchPageComponents.GLASBWrapper"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HitomiScrollViewerLib.Controls.SearchPageComponents"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <AutoSuggestBox x:Name="AutoSuggestBox" x:FieldModifier="internal"
                    QueryIcon="Find"
                    GotFocus="AutoSuggestBox_GotFocus"
                    TextChanged="AutoSuggestBox_TextChanged"
                    QuerySubmitted="AutoSuggestBox_QuerySubmitted"
                    DisplayMemberPath="DisplayName"
                    TextMemberPath="DisplayName"
                    IsEnabled="False"
                    />
</StackPanel>
